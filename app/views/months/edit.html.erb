<%= render "components/dashboard_window" do %>
  <h1 class="font-bold text-4xl">Editing month</h1>

  <%= render "form", month: @month %>

  <%= render partial: "components/gray_button", locals: { message: 'Show this month', path: @month } %>
  <%= render partial: "components/gray_button", locals: { message: 'Back to months', path: team_months_path(@month.team) } %>
<% end %>
